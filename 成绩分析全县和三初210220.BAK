******统计临沭县各校or三初各班的各段成绩\名次的人数******
*
*	20210220 以三初统计为底，添加全县分析内容，试整合成一个程序
*
*  先运行本程序，在提示后打开成绩表
*	建档数在程序最末，直接下拉查找模块pjtjktc
*
*	本程序同时计算学科成绩和班级成绩，当前按7年级1-8班，8年级1-4班为智慧班
*	每次需确定建档数和智慧班
**********************************************
set talk off
clear 
	if .not. directory('e:\cjfx')		 &&创建目录“成绩分析”
		md e:\cjfx
		? '新建目录e:\cjfx，文件存放于此目录'
	endif
	set default to e:\cjfx				&&默认目录
		? '文件默认存放于e:\cjfx'
messagebox('确认各字段命名均为汉字，特别注意：姓名、考号、班级、道法、总成绩、学校。――――点确定后选择成绩数据库',1)
	use ? in 1		&&打开成绩表
	sele 1
	go top
	nj=int(val(substr(考号,3,1))+6)*100 &&获取年级为700，800，900
	 
	  xz='3'  &&选择1为全县，3为三初
		     
		      @8,20 SAY '选择分析目标：1、全县，2、三初:' get xz &&*********************************************************
		    
		      read
		fxmb=val(xz) &&分析目标

	
creat table bzk(姓名 c(8),考号 c(9),班级 c(8), 语文 n(8),语文名次 n(8),数学 n(8),数学名次 n(8), ;
	英语 n(8),英语名次 n(8),物理 n(8),物理名次 n(8),化学 n(8),化学名次 n(8),道法 n(8),道法名次 n(8), ;
	历史 n(8),历史名次 n(8),地理 n(8),地理名次 n(8),生物 n(8),生物名次 n(8), ;
	总成绩 n(8),班级排名 n(8),学校排名 n(8),年级排名 n(8),学校 c(8) ) &&标准库，与县析相同
creat table tjk(学校 c(8),班级 c(8),总分上 n(8),总人数 n(8),总成绩 n(8,2),语文 n(8,2),数学 n(8,2),英语 n(8,2), ;
	物理 n(8,2),化学 n(8,2),道法 n(8,2),历史 n(8,2),地理 n(8,2),生物 n(8,2),进线 c(8) ,总分率 n(8,2),;
	语文率 n(8,2),数学率 n(8,2),英语率 n(8,2), 物理率 n(8,2),化学率 n(8,2),;
	道法率 n(8,2),历史率 n(8,2),地理率 n(8,2),生物率 n(8,2),校名 c(8) ) &&统计库,县析增加各种率

creat table mck(mc n(8),mcd n(8)) &&名次库,仅县析用
	if fxmb=1
		insert into mck(mc) value (10)
		insert into mck(mc) value (50)
		insert into mck(mc) value (100)
		insert into mck(mc) value (200)
		insert into mck(mc) value (500)
		insert into mck(mc) value (700)
		insert into mck(mc) value (1000) 
		insert into mck(mc) value (1500)
		insert into mck(mc) value (2000)
		insert into mck(mc) value (2500)
		insert into mck(mc) value (3000)
		insert into mck(mc) value (3500)
		insert into mck(mc) value (4000)
		insert into mck(mc) value (4500) 
	else
		zd='1000'
		 @12,20 SAY '请输入1000名对应查询人数:' get zd
		  @14,20 SAY '请输入4500名对应查询人数:' get zd1
		 read
		名次1=val(zd)
		insert into mck(mc,mcd) value (名次1,1000)
		名次1=val(zd1)
		insert into mck(mc,mcd) value (名次1,4500)
	endif

creat table pjtjk(班级 c(3),建档数 n(8),特殊减少 n(8) ,考评人数 n(8,0),平均分 n(8,3), ;
		语文 n(8,3),数学 n(8,3),英语 n(8,3),物理 n(8,3),化学 n(8,3),道法 n(8,3),历史 n(8,3),地理 n(8,3),生物 n(8,3),;
		单位 c(8),代码 c(2),最高分 n(8),人数98 n(8,0),平均分98 n(8,3),平均98名次 n(8,0)	) &&平均分统计库,
		&&	上面第三行为县析用，后期注意把人数98替换为考评人人数，平均分98及平均98之后的98均不显示，县析时考评人数=建档数*0.98


do pjtjktc with nj,fxmb 	&&平均统计库填充，做成过程
	** 将平均库放在最后，方便更新数据时直接下接找到

	if fxmb=1		&&	县析用98折，校析用减
			replace all 人数98 with 建档数*0.98
		else
			replace all 考评人数 with 建档数-int(nj/100)+6-特殊减少 &&年级7为减1人，8为减2人，9为减3人
	endif
	
creat table bjfx(班级 c(8),第1 n(8),前10 n(8),前20 n(8),前1000 n(8),前4500 n(8),平均分 n(8,3), ;
	奖10 n(8),奖45 n(8),名次10 n(8),名次45 n(8),名次平均 n(8),名次和 n(10 ,6),排名 n(8),赋分 n(8,2) ) &&班级分析库
	append from pjtjk fields 班级		

